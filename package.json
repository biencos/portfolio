{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.30.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --env=jsdom",
    "test:coverage": "craco test --env=jsdom --coverage --watchAll=false",
    "test:ci": "craco test --env=jsdom --coverage --watchAll=false --ci",
    "eject": "craco eject",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "check": "npm run lint && npm run format:check",
    "validate": "npm run check && npm run build",
    "pre-push": "npm run validate",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss,json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@craco/craco": "^7.1.0",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "react-scripts": "^5.0.1"
  },
  "overrides": {
    "@typescript-eslint/eslint-plugin": {
      "eslint": "^9.33.0"
    },
    "@typescript-eslint/parser": {
      "eslint": "^9.33.0"
    },
    "@typescript-eslint/utils": {
      "eslint": "^9.33.0"
    },
    "nth-check": "^2.1.1",
    "postcss": "^8.4.49",
    "svgo": "^3.3.2",
    "@humanwhocodes/config-array": "npm:@eslint/config-array@^0.19.0",
    "@humanwhocodes/object-schema": "npm:@eslint/object-schema@^2.1.5",
    "rimraf": "^6.0.1",
    "glob": "^11.0.0",
    "inflight": "^1.0.6",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0",
    "source-map": "^0.7.4",
    "eslint": "^9.33.0",
    "@babel/plugin-proposal-class-properties": "npm:@babel/plugin-transform-class-properties@^7.26.0",
    "@babel/plugin-proposal-private-methods": "npm:@babel/plugin-transform-private-methods@^7.26.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "npm:@babel/plugin-transform-nullish-coalescing-operator@^7.26.0",
    "@babel/plugin-proposal-numeric-separator": "npm:@babel/plugin-transform-numeric-separator@^7.26.0",
    "@babel/plugin-proposal-optional-chaining": "npm:@babel/plugin-transform-optional-chaining@^7.26.0",
    "@babel/plugin-proposal-private-property-in-object": "npm:@babel/plugin-transform-private-property-in-object@^7.26.0",
    "rollup-plugin-terser": "npm:@rollup/plugin-terser@^0.4.4",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-google-analytics": "^7.3.0",
    "source-map-loader": "^5.0.0",
    "domexception": "^4.0.0",
    "babel-plugin-istanbul": "^7.0.0",
    "test-exclude": "^7.0.1"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/coverage/",
      "src/index.js",
      "src/setupTests.js",
      "src/utils/testUtils.js",
      "\\.css$",
      "craco.config.js"
    ]
  }
}
